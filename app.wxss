.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.rich-text {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: burlywood;
  margin-top: 20px;
  font-size: 13px;
  text-decoration: underline
}

.error {
  display: flex;
  padding: 10px;
  position: relative;
  text-align: center;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 20px;
}
